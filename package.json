{
  "name": "next_initial_frame",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 9030",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ext \".ts,.js,.tsx,.jsx\" --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext \".ts,.js,.tsx,.jsx\" --ignore-path .gitignore .",
    "prettier": "prettier --write .",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
